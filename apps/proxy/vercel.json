{
  "rewrites": [
    {
      "source": "/",
      "destination": "https://thoughtbase-landing.vercel.app/"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www\\..*"
        }
      ],
      "destination": "https://thoughtbase-app.vercel.app/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<subdomain>.*)\\..*"
        }
      ],
      "destination": "https://:subdomain.thoughtbase-app.vercel.app/:path*"
    },
    {
      "source": "/:path*",
      "destination": "https://thoughtbase-app.vercel.app/:path*"
    }
  ]
}
